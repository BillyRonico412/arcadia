{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
		"enabled": false,
		"clientKind": "git",
		"useIgnoreFile": false
	},
	"files": {
		"ignoreUnknown": false,
		"ignore": []
	},
	"formatter": {
		"enabled": true,
		"indentStyle": "tab"
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"all": true,
			"style": {
				"all": true,
				"noDefaultExport": "off"
			},
			"suspicious": {
				"all": true,
				"noReactSpecificProps": "off"
			},
			"nursery": {
				"all": true,
				"useExplicitType": "off"
			},
			"a11y": {
				"all": true
			},
			"complexity": {
				"all": true
			},
			"correctness": {
				"all": true
			},
			"performance": {
				"all": true
			},
			"security": {
				"all": true
			}
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "double",
			"semicolons": "asNeeded"
		}
	}
}